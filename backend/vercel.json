{
    "version": 2,
    "builds": [
        { "src": "*.js", "use": "@vercel/node" }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/",
            "methods": ["GET","OPTIONS","PATCH","DELETE","POST","PUT"],
            "headers": {
                "Access-Control-Allow-Origin": "*"
            }
        }
    ]
}